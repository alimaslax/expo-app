{
  "name": "expo-app",
  "description": "Bulletproof Expo SDK 49 Template",
  "version": "1.0.0",
  "author": {
    "name": "Tri Rizeki Rifandani",
    "email": "tri.rifandani@gmail.com"
  },
  "private": false,
  "main": "expo-router/entry",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write src/",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "regression": "run-p lint typecheck",
    "i18n": "typesafe-i18n",
    "config:introspect": "expo config --type introspect",
    "install:fix": "expo install --fix",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "dev": "expo start",
    "dev:clean": "expo start -c",
    "start": "run-p i18n dev",
    "start:clean": "run-p i18n dev:clean",
    "start:prod": "run-p i18n dev:prod",
    "build:android": "eas build --profile development --platform android",
    "build:android:local": "eas build --profile development --platform android --local",
    "build:ios": "eas build --profile development-simulator --platform ios",
    "build:ios:local": "eas build --profile development-simulator --platform ios --local",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "release": "bumpp"
  },
  "dependencies": {
    "@dev-plugins/react-navigation": "^0.3.1",
    "@dev-plugins/react-query": "^0.3.1",
    "@expo/metro-config": "~0.20.17",
    "@expo/vector-icons": "^14.1.0",
    "@formatjs/intl-getcanonicallocales": "^2.5.5",
    "@formatjs/intl-locale": "^4.2.11",
    "@formatjs/intl-pluralrules": "^5.4.4",
    "@hookform/resolvers": "^5.2.1",
    "@rifandani/nxact-yutiriti": "^1.2.2",
    "@tamagui/animations-moti": "^1.132.19",
    "@tamagui/babel-plugin": "^1.132.19",
    "@tamagui/config": "^1.132.19",
    "@tamagui/theme-builder": "^1.132.19",
    "@tamagui/toast": "^1.132.19",
    "@tanstack/react-query": "^5.85.3",
    "axios": "^1.7.4",
    "burnt": "^0.13.0",
    "expo": "~53.0.20",
    "expo-application": "~6.1.5",
    "expo-video": "^2.2.2",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "ky": "^1.8.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^3.3.0",
    "react-native-reanimated": "^3.19.1",
    "react-native-safe-area-context": "5.6.0",
    "react-native-screens": "~4.14.1",
    "tamagui": "^1.132.19",
    "type-fest": "^4.41.0",
    "typesafe-i18n": "^5.26.2",
    "zod": "^4.0.17",
    "zod-validation-error": "^4.0.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "bumpp": "^10.2.3",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-universe": "^15.0.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "packageManager": "yarn@1.22.22"
}